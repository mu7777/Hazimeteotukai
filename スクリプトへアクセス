while true do
if gg.isVisible(true) then
Lext=1
gg.setVisible(false)
end

function Main()
Ax = gg.choice({
"ジャンル分けスクリプトを実行する。",
"終了"}, nil, "スクリプトアクセスv.1 (更新日2024/04/19)")

if Ax ~= nil then

if Ax == 1 then
gg.alert("メンテナンス中です。\n最新情報はDiscordから")
end

if Ax == 2 then
os.exit()
end
end
Lext=-1
end

if Lext == 1 then
Main()
end
end
